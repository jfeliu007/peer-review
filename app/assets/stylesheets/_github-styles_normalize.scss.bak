    .previewable-comment-form .comment-form-head.tabnav {
      color: #8e9597;
      background-color: #f2f8fa;
      border-bottom-color: #e1edf1;
    }
  }
  &:empty {
    display: none;
  }
  .comment + .comment {
    border-top: 1px solid #e5e5e5;
    &:before, &:after {
      display: none;
    }
    .timeline-comment-header {
      border-top-left-radius: 0;
      border-top-right-radius: 0;
