.date_selector {
  width: 225px;
  text-align: left;
  text-decoration: none;
  z-index: 9;
  display: none;
  .month_nav, .year_nav {
    margin-top: 5px;
    margin-bottom: 5px;
    padding: 0;
    display: block;
    position: relative;
    text-align: center;
    line-height: 20px;
  }
  .month_nav {
    float: left;
    width: 55%;
  }
  .year_nav {
    float: right;
    width: 35%;
  }
  .date-button {
    position: absolute;
    top: 0;
    width: 18px;
    height: 18px;
    padding: 4px;
    color: #4078c0;
    font-size: 12px;
    cursor: pointer;
    line-height: 12px;
  }
  .prev {
    left: 0;
  }
  .next {
    right: 0;
  }
  table {
    width: 100%;
    clear: both;
  }
  tr {
    font-size: 0;
  }
  th {
    width: 32px;
    height: 32px;
    line-height: 28px;
    padding: 0;
    text-align: center;
    font-weight: normal;
    display: inline-block;
    font-size: 12px;
    margin-top: -1px;
    margin-left: -1px;
  }
  td {
    width: 32px;
    height: 32px;
    line-height: 28px;
    padding: 0;
    text-align: center;
    font-weight: normal;
    display: inline-block;
    font-size: 12px;
    margin-top: -1px;
    margin-left: -1px;
    border: 1px solid #ccc;
    color: #4078c0;
    background: #fff;
    cursor: default;
    &.today {
      background: #eee;
    }
    &.selected, &.selectable_day:hover {
      background: #4078c0;
      color: #fff;
      border-color: #33609a;
      z-index: 10;
      position: relative;
      cursor: pointer;
    }
    &.unselected_month {
      color: #ccc;
    }
  }
}

.jcrop-holder {
  direction: ltr;
  text-align: left;
  touch-action: none;
}

.jcrop-vline, .jcrop-hline {
  background: #fff url("/images/spinners/Jcrop.gif");
  font-size: 0;
  position: absolute;
}

.jcrop-vline {
  height: 100%;
  width: 1px !important;
  &.right {
    right: 0;
  }
}

.jcrop-hline {
  height: 1px !important;
  width: 100%;
  &.bottom {
    bottom: 0;
  }
}

.jcrop-tracker {
  height: 100%;
  width: 100%;
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

.jcrop-handle {
  background-color: #333;
  border: 1px #eee solid;
  width: 7px;
  height: 7px;
  font-size: 1px;
  &.ord-n {
    left: 50%;
    margin-left: -4px;
    margin-top: -4px;
    top: 0;
  }
  &.ord-s {
    bottom: 0;
    left: 50%;
    margin-bottom: -4px;
    margin-left: -4px;
  }
  &.ord-e {
    margin-right: -4px;
    margin-top: -4px;
    right: 0;
    top: 50%;
  }
  &.ord-w {
    left: 0;
    margin-left: -4px;
    margin-top: -4px;
    top: 50%;
  }
  &.ord-nw {
    left: 0;
    margin-left: -4px;
    margin-top: -4px;
    top: 0;
  }
  &.ord-ne {
    margin-right: -4px;
    margin-top: -4px;
    right: 0;
    top: 0;
  }
  &.ord-se {
    bottom: 0;
    margin-bottom: -4px;
    margin-right: -4px;
    right: 0;
  }
  &.ord-sw {
    bottom: 0;
    left: 0;
    margin-bottom: -4px;
    margin-left: -4px;
  }
}

.jcrop-dragbar {
  &.ord-n, &.ord-s {
    height: 7px;
    width: 100%;
  }
  &.ord-e, &.ord-w {
    height: 100%;
    width: 7px;
  }
  &.ord-n {
    margin-top: -4px;
  }
  &.ord-s {
    bottom: 0;
    margin-bottom: -4px;
  }
  &.ord-e {
    margin-right: -4px;
    right: 0;
  }
  &.ord-w {
    margin-left: -4px;
  }
}

.jcrop-light {
  .jcrop-vline, .jcrop-hline {
    background: #fff;
    opacity: 0.7 !important;
  }
  .jcrop-handle {
    background-color: #000;
    border-color: #fff;
    border-radius: 3px;
  }
}

.jcrop-dark {
  .jcrop-vline, .jcrop-hline {
    background: #000;
    opacity: 0.7 !important;
  }
  .jcrop-handle {
    background-color: #fff;
    border-color: #000;
    border-radius: 3px;
  }
}

.jcrop-holder img, img.jcrop-preview {
  max-width: none;
}

.axis {
  fill: #aaa;
  font-size: 10px;
  line {
    shape-rendering: crispedges;
    stroke: #eee;
  }
  path {
    display: none;
  }
  .zero line {
    stroke-width: 1.5;
    stroke: #4078c0;
    stroke-dasharray: 3 3;
  }
}

.graphs {
  .is-graph-loading {
    min-height: 500px;
  }
  &.wheader h2 {
    padding: 1px;
  }
  .area {
    fill: #1db34f;
    fill-opacity: 0.5;
  }
  .path {
    stroke: #1db34f;
    stroke-width: 2px;
    stroke-opacity: 1;
    fill: none;
  }
  .dot {
