<div class="row">
	<div class="col-lg-12">
   		<h1 class="page-header">Your Organizations</h1>
  	</div>
	<%@orgs.each do |org|%>
    	<div class="col-lg-3 col-md-4 col-xs-6 thumb">
			<%= link_to image_tag(org.avatar_url,:width => "200px"), repos_path(:org_name => org.login), :class => 'thumbnail' %>
     	</div>
	<% end %>
</div>
